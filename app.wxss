/**app.wxss**/
@import 'zanui.wxss';  
/* 定义全局样式 */
page{
  background-color: #f5f6f7;
  width: 750rpx;
  height:100%;
  overflow: hidden;
  font-size: 32rpx;
  font-weight: 400;
}


text {
  color: rgb(51, 51, 51);
}

.flex_cen{
  display: flex;
  align-items: center;
  justify-content: center;
}
.clear{
  clear: both;
}

::-webkit-scrollbar {
  display: none;
}

.blank_bottom {
  height: 20rpx;
}

/* 背景 */
.bg_layer {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background-color: #f5f6f7;
  z-index: -100000;
}