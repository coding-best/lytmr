  </view>

  <view class="content">
    <view class="content_list">
      <view class="icon"></view>
      <text>报名表单填写</text>
    </view>
    <view class="content_list">
      <text>姓名<text class="star">*</text></text>
      <input type="text" placeholder="" placeholder-class="placeholder" />
    </view>
    <view class="content_list">
      <text>手机<text class="star">*</text></text>
      <input type="text" placeholder="" placeholder-class="placeholder" />
    </view>
  </view>
  <view class="next" bindtap="hrefActivitySignSubmit">提交报名</view>
</view>