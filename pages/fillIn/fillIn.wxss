input, picker  {
  height: 100%;
  width: 320rpx;
}
.picker {
  height: 80rpx;
  width: 320rpx;
}

.container_content {
    width: 100%;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.content_list {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    height: 90rpx;
    border-bottom: 1rpx solid #efefef;
    line-height: 90rpx;
    padding-left: 50rpx;
    font-size: 24rpx;
}

.content_list>text:first-child {
    display: block;
    width: 176rpx;
    color: rgb(102, 102, 102);
}

.content_list>text:last-child {
    color: rgb(51, 51, 51);
}

.content_list_title {
    display: flex;
    align-items: center;
    padding-left: 0;
}

.content_list_title>text {
    padding-right: 6rpx;
    color: rgb(51, 51, 51);
}

.content_list_title>text:last-child {
    color: rgb(243, 75, 67)
}

.error {
    width: 26rpx;
    height: 26rpx;
    background-image: url('https://ofdznzfo9.qnssl.com/szdx_errorIcon.pic');
    background-size: cover;
    margin-right: 6rpx;
}

.error_text {
    font-size: 22rpx;
}

.title_img {
    width: 22rpx;
    height: 26rpx;
    background-size: cover;
    margin: 0 11rpx 2rpx 17rpx;
}

.content_switch {
    display: flex;
    justify-content: space-between;
}

.content_switch>switch {
    margin-right: 40rpx;
}

.next {
    width: 670rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 50rpx;
    background-color: #3c98e7;
    color: #fff;
    margin: 40rpx;
    font-size: 26rpx;
}

